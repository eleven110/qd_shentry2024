<launch>
    <!-- 运行地图服务器，并且加载设置的地图-->
    <include file="$(find qidian_tactics)/launch/file/reader_map.launch" />

    <!-- amcl定位 -->
    <include file="$(find qidian_tactics)/launch/file/amcl.launch" />

    <!-- 运行move_base节点 -->
    <include file="$(find qidian_tactics)/launch/file/nav_amcl_path.launch" />
    
    <!-- 运行rviz -->
    <node pkg = "rviz" type ="rviz" name="rviz" args="-d $(find qidian_tactics)/config/amcl.rviz"/>


    <node pkg = "joint_state_publisher" type = "joint_state_publisher"  name= "joint_state_publisher" />

    <node pkg = "robot_state_publisher" type = "robot_state_publisher"  name= "robot_state_publisher" />
</launch>
